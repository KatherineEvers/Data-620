   "source": [
    "# Assignment 1: “hello, graph world”\n",
    "\n",
    "Objective: \n",
    "Create the nodes and edges for a basic graph such as the Krackhardt kite "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "import networkx as nx\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "% matplotlib inline"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "edges = {\"Jane\" : [\"Ike\"], \n",
    "         \"Ike\" : [\"Jane\", \"Heather\"], \n",
    "         \"Heather\": [\"Ike\", \"Fernando\", \"Garth\"],\n",
    "         \"Fernando\": [\"Heather\", \"Garth\", \"Diane\", \"Andre\", \"Carol\"],\n",
    "         \"Garth\": [\"Heather\", \"Fernando\", \"Diane\", \"Beverly\", \"Ed\"],\n",
    "         \"Diane\": [\"Fernando\", \"Garth\", \"Carol\", \"Andre\", \"Beverly\", \"Ed\"],\n",
    "         \"Carol\": [\"Fernando\", \"Diane\", \"Andre\"],\n",
    "         \"Andre\": [\"Fernando\", \"Diane\", \"Carol\", \"Beverly\"],\n",
    "         \"Beverly\": [\"Andre\", \"Diane\", \"Garth\", \"Ed\"],\n",
    "         \"Ed\": [\"Beverly\", \"Diane\", \"Garth\"]\n",
    "        }\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "colors = ['r', 'c', 'r', 'm', 'g', 'm', 'm', 'g', 'g', 'c']"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "g = nx.from_dict_of_lists(edges)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 432x288 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "nx.draw(g, with_labels=True, node_color=colors, node_shape='D')"
   ]
  },

